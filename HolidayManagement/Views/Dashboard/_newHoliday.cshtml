<form>

@if (User.IsInRole("Admin") || User.IsInRole("HR"))
{
    <div class="form-group">
        <label for="sel1">Email:</label>
       
            <select class="form-control" id="sel1" data-bind="options : users,
                                   optionsText : 'email',
                                   optionsValue : 'id',
                                   value : manageUser.id ,
                                   optionsCaption : 'Choose...'
                                   "></select>
        
       
    </div>
}
    <span data-bind="text: errmes" ></span>
    <div class="form-group">
        <label >From Date:</label>
        <input type="date" class="form-control" data-bind="value: fromDate" />
    </div>

    <div class="form-group">
        <label >To Date:</label>
        <input type="date" class="form-control" id="toDate" data-bind="value: toDate" />
    </div>


</form>
